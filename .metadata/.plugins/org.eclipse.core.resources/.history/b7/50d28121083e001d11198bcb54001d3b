package sec3;

public class CarEX {

	public static void main(String[] args) {
		Car car1 = new Car();
		
		for(int i=1;i<=10;i++){
			int errorLocation = car1.run();
			
			switch(errorLocation){
				case 1:
					System.out.println("앞쪽 왼쪽 바퀴Michelin Tire로 교체");
					car1.frontLeftTire = new MichelinTire(12,"앞왼쪽");
			}
		}
	}

}
