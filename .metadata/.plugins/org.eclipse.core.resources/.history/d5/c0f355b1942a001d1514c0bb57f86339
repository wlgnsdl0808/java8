package sec2;

public class Cast2 {

	public static void main(String[] args) {
		int a= 100, b=23, c;
		float d, e, f;
		c = a / b;		// -> 자동 형 변환
		System.out.println("c="+c);
		d = (float) a / b;		// 4.0 -> 강제 형 변환
		System.out.println("d="+d);
		e = 500 / 3.0f;
		System.out.println("e="+e);

	}

}
