package sec;

public class ChildEx1 {

	public static void main(String[] args) {
		Child c1 = new Child();
		c1.field1 = 10;
		c1.field2 = "kim";
		c1.field3 = 20;
		
		
		Parent c2 = new Parent();
		c2.field1 = 100;
		c2.field2 = "lee";
		c2.field2 = 200;
		
		//부모 클래스로 부터 유도된 객체는 부모의 생성자 또는 자식의 생성자를 활용하여 객체를 생성할 수 있다.
		Parent c3;
		c3 = new Parent();
		c3.method1();
		c3.method2();
		
		//그러나 자식의 맴버는 활용될 수 없다.
		c3 = new Child();
		//c3.method3();
		//c3.method4();

	}

}
