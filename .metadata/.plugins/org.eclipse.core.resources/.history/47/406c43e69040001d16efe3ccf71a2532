package sec2;

public class SystemEx3 {

	public static void main(String[] args) {
		long time1 = System.nanoTime();
		
		long sum=0;
		for(int i=1;1<10000000;i++){
			sum+=i;
		}
		
		long time2 = System.nanoTime();
		
		System.out.println("합산 결과 : "+sum);
		System.out.println("걸린 시간 : "+(time2-time1));
	}

}
