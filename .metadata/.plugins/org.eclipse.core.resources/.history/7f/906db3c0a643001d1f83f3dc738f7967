package sec4;

public class StringEx3 {

	public static void main(String[] args) {
		String str1 = new String("");
		str1+="김지은";
		str1+=" ";
		str1+="박평화";
		str1+=" ";
		str1+="강병수";
		System.out.println("출력 1 : "+str1);
		
		StringBuffer str2 = new StringBuffer();
		str2.append("김지은");
		str2.append(" ");
		str2.append("박평화");
		str2.append(" ");
		str2.append("강병수");
		System.out.println("출력 2 : "+str2.toString());

	}

}
